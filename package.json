{
  "name": "premake-manager",
  "displayName": "premake manager",
  "description": "a extension to manage premake projects",
  "version": "0.0.1",
  "type": "commonjs",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "category": "Premake",
        "command": "premake.action",
        "title": "run premake action"
      },
      {
        "category": "Premake",
        "command": "premake.setversion",
        "title": "set premake version"
      }
    ],
    "configuration": {
      "type": "object",
      "properties": {
        "premake.version": {
          "type": "string",
          "default": "latest",
          "description": "Choose the version of Premake to use for this project."
        },
        "premake.repository": {
          "type": "string",
          "default": "https://github.com/premake/premake-core",
          "description": "the url from wich to get the premake versions from."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@types/vscode": "^1.96.0",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "axios": "^1.7.9",
    "chokidar": "^4.0.3",
    "eslint": "^9.13.0",
    "ts-loader": "^9.5.1",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "typescript": "^5.6.3",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@octokit/core": "^6.1.2",
    "@octokit/rest": "^21.0.2",
    "@octokit/types": "^13.6.2"
  }
}
