{
  "name": "premake-manager",
  "displayName": "premake manager",
  "description": "a extension to manage premake projects",
  "version": "0.1.3",
  "type": "commonjs",
  "publisher": "lolrobbe2",
  "engines": {
    "vscode": "^1.92.1"
  },
  "categories": [
    "Other",
    "Snippets",
    "Linters",
    "Formatters"
  ],
  "keywords": [
    "premake",
    "autocomplete",
    "install",
    "snippets",
    "libraries",
    "modules"
  ],
  "author": {
    "name": "robbe beernaert"
  },
  "license": "MIT",
  "icon": "resources/media/premake-logo-128.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/lolrobbe2/premake-manager.git"
  },
  "bugs": {
    "url": "https://github.com/lolrobbe2/premake-manager/issues"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "terminal": {
      "profiles": [
        {
          "id": "premake5.terminal-profile",
          "title": "Premake Manager"
        },
        {
          "id": "premake5.environment-profile",
          "title": "Premake5"
        }
      ]
    },
    "snippets": [
      {
        "language": "jsonc",
        "path": "./snippets/premake5.code-snippets"
      },
      {
        "language": "lua",
        "path": "./snippets/premake5.workspace.code-snippets"
      },
      {
        "language": "lua",
        "path": "./snippets/premake5.filter.code-snippets"
      },
      {
        "language": "lua",
        "path": "./snippets/premake5.project.code-snippets"
      }
    ],
    "taskDefinitions": [],
    "commands": [
      {
        "category": "Premake5",
        "command": "premake5.manager-cli",
        "title": "Open Premake5 Manager CLI Terminal"
      },
      {
        "category": "Premake5",
        "command": "premake5.version-set",
        "title": "Set the active premake5 version ",
        "when": "false"
      },
      {
        "category": "Premake5",
        "command": "premake5.version-list-installed",
        "title": "List all the installed premake version",
        "when": "false"
      },
      {
        "category": "Premake5 version",
        "command": "premake5.version-list-releases",
        "title": "List alle the available releases",
        "when": "false"
      },
      {
        "category": "premake manager",
        "command": "premake5.version-group",
        "title": "version commands",
        "icon": "resources/media/premake-logo-128.png"
      },
      {
        "category": "Premake5",
        "command": "premake5.module-info",
        "title": "return the info from a premake module",
        "icon": "resources/media/premake-logo-128.png",
        "when": "false"
      },
      {
        "category": "Premake5",
        "command": "premake5.module-install",
        "title": "install a premake module",
        "icon": "resources/media/premake-logo-128.png",
        "when": "false"
      },
      {
        "category": "Premake5",
        "command": "premake5.module-remove",
        "title": "remove a premake module",
        "icon": "resources/media/premake-logo-128.png",
        "when": "false"
      },
      {
        "category": "premake manager",
        "command": "premake5.module-group",
        "title": "module commands",
        "icon": "resources/media/premake-logo-128.png",
        "when": "false"
      },

       {
        "category": "Premake5",
        "command": "premake5.library-info",
        "title": "return the info from a premake library",
        "icon": "resources/media/premake-logo-128.png",
        "when": "false"
      },
      {
        "category": "Premake5",
        "command": "premake5.library-install",
        "title": "install a premake library",
        "icon": "resources/media/premake-logo-128.png",
        "when": "false"
      },
      {
        "category": "Premake5",
        "command": "premake5.library-remove",
        "title": "remove a premake library",
        "icon": "resources/media/premake-logo-128.png",
        "when": "false"
      },
      {
        "category": "premake manager",
        "command": "premake5.library-group",
        "title": "library commands",
        "icon": "resources/media/premake-logo-128.png",
        "when": "false"
      },
      {
        "category": "premake manager",
        "command": "premake5.config-group",
        "title": "config commands",
        "icon": "resources/media/premake-logo-128.png",
        "when": "false"
      },
      {
        "category": "premake manager",
        "command": "premake5.config-view",
        "title": "view config",
        "icon": "resources/media/premake-logo-128.png",
        "when": "false"
      },
      {
        "category": "premake manager",
        "command": "premake5.configure",
        "title": "configure",
        "icon": "resources/media/premake-logo-128.png",
        "when": "false"
      },
      {
        "category": "Premake5",
        "command": "premake5.manager",
        "icon": "resources/media/premake-logo-128.png",
        "title": "manager"
      },
      {
        "category": "Premake5",
        "command": "premake5.environment-cli",
        "icon": "resources/media/premake-logo-128.png",
        "title": "environment terminal"
      }
    ],
    "keybindings": [
      {
        "command": "premake5.manager-cli",
        "key": "ctrl+alt+p",
        "when": "terminalFocus == false"
      },
      {
        "command": "premake5.manager",
        "key": "ctrl+alt+m"
      },
      {
        "command": "premake5.environment-cli",
        "key": "ctrl+alt+t",
        "when": "terminalFocus == false"
      }
    ],
    "menus": {},
    "configuration": {
      "type": "object",
      "properties": {}
    }
  },
  "scripts": {
    "clean": "rimraf out && rimraf tsconfig.extension.tsbuildinfo",
    "compile": "npm run clean && node build.js",
    "vscode:prepublish": "npm run compile",
    "watch": "npm run compile && tsc -watch -p tsconfig.extension.json",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "tsc:w": "node --version",
    "resources": "cp -rv ./src/resources/** ./out/resources",
    "icons": "fantasticon ./src/resources/media/ -o ./src/resources/media/",
    "meta": "node docs/metagen.mjs"
  },
  "dependencies": {
    "@types/vscode": "^1.96.0",
    "axios": "^1.7.9",
    "chokidar": "^4.0.3",
    "cp": "^0.2.0",
    "tar": "^7.4.3"
  },
  "devDependencies": {
    "@octokit/types": "^13.6.2",
    "@types/adm-zip": "^0.5.7",
    "@types/mocha": "^10.0.9",
    "@types/node": "^20.9.0",
    "@types/vscode": "^1.92.1",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "@vscode/vsce": "^3.2.1",
    "esbuild": "^0.24.2",
    "eslint": "^9.13.0",
    "rimraf": "^5.0.10",
    "typescript": "^5.7.2"
  }
}
